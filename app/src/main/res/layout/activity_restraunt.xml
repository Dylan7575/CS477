    android:layout_height="fill_parent" android:background="#ffffff">

    <TextView android:id="@+id/restrauntText" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:textColor="#000000"
        android:textSize="24dip" android:layout_margin="5dip" android:text="Restaraunts"></TextView>
    <ListView android:layout_height="match_parent"
        android:id="@+id/ListViewRestaraunt"
        android:layout_width="367dp" android:background="#ffffff"
